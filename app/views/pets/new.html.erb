<h1>Create a new pet!</h1>
<%= form_for(@pet) do |f| %>
    <label> Name </label>
    <%= f.text_field :name %> <br><br>
    <label> Age </label>
    <%= f.number_field :age %> <br><br>
    <label> Type</label>
    <%= f.text_field :type%> <br><br>
    <label> Breed</label>
    <%= f.text_field :breed%> <br><br>
    <label>Choose your shelter</label>
    <%= f.collection_select :name, @shelters, :name, :name %>
    <%= f.submit %>
<%end%>