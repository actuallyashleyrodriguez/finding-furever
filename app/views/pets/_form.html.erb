<%= form_for(@pet) do |f| %>
    <label> Name </label>
    <%= f.text_field :name %> <br><br>
    <label> Age </label>
    <%= f.number_field :age %> <br><br>
    <label> Type of Animal</label>
    <%= f.text_field :animal_type %> <br><br>
    <label> Breed</label>
    <%= f.text_field :breed%> <br><br>
    <label>Choose your Shelter</label>
    <%= f.text_field :shelter_name, list: "shelters_autocomplete" %>
    <datalist id="shelters_autocomplete">
        <% Shelter.all.each do |shelter| %>
            <option value="<%=shelter.name%>">
        <%end %>
    </datalist>
    <%= f.submit %>
<%end%>