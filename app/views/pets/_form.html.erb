<%= form_for(@pet) do |f| %>
    <label> Name </label>
    <%= f.text_field :name %> <br><br>
    <label> Age </label>
    <%= f.number_field :age %> <br><br>
    <label> Type of Animal</label>
    <%= f.text_field :animal_type %> <br><br>
    <label> Breed</label>
    <%= f.text_field :breed%> <br><br>
    <label>Choose your Shelter</label>
    <%= f.collection_select :shelter_name, @shelters, :name, :name %>
    <%= f.submit %>
<%end%>